<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.github.phpmaven.test</groupId>
	<artifactId>test-with-dep2</artifactId>
	<packaging>php</packaging>
	<name>Sample PHP 5 library project</name>
	<version>0.0.1</version>
	
	<parent>
		<groupId>org.github.phpmaven</groupId>
		<artifactId>php-parent-pom</artifactId>
		<version>3.0.0-SNAPSHOT</version>
	</parent>
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.github.phpmaven</groupId>
				<artifactId>maven-php-plugin</artifactId>
				<version>3.0.0-SNAPSHOT</version>
				<extensions>true</extensions>
			</plugin>
			

			
		</plugins>
	</build>
	<dependencies>

        <dependency>
            <groupId>org.phpunit</groupId>
            <artifactId>phpunit5</artifactId>
            <version>3.6.2</version>
            <scope>test</scope>
        </dependency>
		<dependency>
			<groupId>org.github.phpmaven.test</groupId>
			<artifactId>test-with-dep1</artifactId>
			<version>0.0.1</version>
			<type>phar</type>
		</dependency>
	</dependencies>
	
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
</project>