	</properties>
	<body>
	<release date="2008-10-01" version="1.0-BETA-1"
			description="1. BETA release">
			<action dev="cw" type="add" >
				Add support for phpdocumentor.
			</action>
			<action dev="cw" type="fix" >
				PHPUnit Maven Wrapper bugfix. 
			</action>
			<action dev="cw" type="add">
